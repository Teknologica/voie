{
  "name": "voie",
  "version": "0.0.0",
  "description": "Voie — simple VueJS router",
  "repository": {
    "type": "git",
    "url": "https://github.com/inca/voie"
  },
  "main": "lib/index.js",
  "dependencies": {
    "debug": "^2.2.0",
    "vue": "^1.0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "mocha": "^2.3.4",
    "pre-commit": "^1.1.2"
  },
  "scripts": {
    "check": "node_modules/eslint/bin/eslint.js .",
    "test": "node_modules/karma/bin/karma start ./test/karma.conf.js",
    "compile": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "prepublish": "npm run compile"
  },
  "pre-commit": [
    "check"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "keywords": [
    "vue",
    "router",
    "layout",
    "ui",
    "browser"
  ],
  "author": "Boris Okunskiy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inca/voie/issues"
  },
  "homepage": "https://github.com/inca/voie"
}
